          <mxGeometry x="720" y="380" width="220" height="180" as="geometry"/>
        </mxCell>

        <!-- RELACIONES -->
        <mxCell id="r1" style="endArrow=block;html=1;" edge="1" parent="1" source="usuario" target="noticia">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="r2" style="endArrow=block;html=1;" edge="1" parent="1" source="usuario" target="evento">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="r3" style="endArrow=block;html=1;" edge="1" parent="1" source="categoria" target="noticia">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

        <mxCell id="r4" style="endArrow=block;html=1;" edge="1" parent="1" source="parroquia" target="evento">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
