<mxfile host="app.diagrams.net">
  <diagram name="ER_GeoNews">
    <mxGraphModel dx="1200" dy="800" grid="1" gridSize="10">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- USUARIO -->
        <mxCell id="usuario" value="Usuario&#10;----------------&#10;PK id&#10;nombre&#10;apellido&#10;email&#10;password&#10;fotoPerfil&#10;bio&#10;telefonoCelular&#10;ubicacion&#10;fechaRegistro&#10;ultimaConexion&#10;tipoUsuario&#10;verificado"
          style="rounded=0;whiteSpace=wrap;html=1;fillColor=#E3F2FD;strokeColor=#1E88E5;"
          vertex="1" parent="1">
          <mxGeometry x="40" y="40" width="260" height="320" as="geometry"/>
        </mxCell>

        <!-- NOTICIA -->
        <mxCell id="noticia" value="Noticia&#10;----------------&#10;PK id&#10;titulo&#10;descripcion&#10;contenido&#10;imagenUrl&#10;autorId (FK)&#10;categoriaId (FK)&#10;fechaCreacion&#10;latitud&#10;longitud&#10;estado"
          style="rounded=0;whiteSpace=wrap;html=1;fillColor=#E8F5E9;strokeColor=#2E7D32;"
          vertex="1" parent="1">
          <mxGeometry x="380" y="40" width="280" height="300" as="geometry"/>
        </mxCell>

        <!-- EVENTO -->
        <mxCell id="evento" value="Evento&#10;----------------&#10;PK id&#10;descripcion&#10;fecha&#10;ubicacion&#10;creadorId (FK)&#10;parroquiaId (FK)&#10;cupoMaximo&#10;cupoActual&#10;estado"
          style="rounded=0;whiteSpace=wrap;html=1;fillColor=#FFF3E0;strokeColor=#EF6C00;"
          vertex="1" parent="1">
          <mxGeometry x="380" y="380" width="280" height="260" as="geometry"/>
        </mxCell>

        <!-- CATEGORIA -->
        <mxCell id="categoria" value="Categoria&#10;----------------&#10;PK id&#10;nombre&#10;descripcion&#10;color"
          style="rounded=0;whiteSpace=wrap;html=1;fillColor=#F3E5F5;strokeColor=#6A1B9A;"
          vertex="1" parent="1">
          <mxGeometry x="720" y="60" width="220" height="180" as="geometry"/>
        </mxCell>

        <!-- PARROQUIA -->
        <mxCell id="parroquia" value="Parroquia&#10;----------------&#10;PK id&#10;nombre&#10;latitud&#10;longitud"
          style="rounded=0;whiteSpace=wrap;html=1;fillColor=#E1F5FE;strokeColor=#0277BD;"
